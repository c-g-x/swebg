{
    "trailingComma": "none",
    "tabWidth": 4,
    "semi": true,
    "printWidth": 200,
    "overrides": [
        {
            "files": ["*.(html|js|vue|md)"],
            "options": {
                "singleQuote": true,
                "quoteProps": "as-needed"
            }
        }
    ],
    "quoteProps": "preserve"
}
